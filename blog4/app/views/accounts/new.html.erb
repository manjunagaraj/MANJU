<%= form_for :account do |f| %>
  <p>
    <%= f.label :ac_id %><br>
    <%= f.text_field :ac_id %>
  </p>
 
  <p>
    <%= f.label :ac_number %><br>
    <%= f.text_area :ac_number%>
  </p>


  <p>
    <%= f.label :supplier_id %><br>
    <%= f.text_area :supplier_id%>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>